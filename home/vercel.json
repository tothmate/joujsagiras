{
  "rewrites": [
    {
        "source": "/hopp",
        "destination": "https://joujsagiras-hopp.vercel.app/hopp"
    },
    {
      "source": "/hopp/:match*",
      "destination": "https://joujsagiras-hopp.vercel.app/hopp/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "rosszujsagiras.hu"
        }
      ],
      "destination": "/hopp",
      "permanent": true
    }
  ]
}
